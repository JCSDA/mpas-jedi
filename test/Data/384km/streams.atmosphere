<streams>
<immutable_stream name="input"
                  type="input"
                  filename_template="./Data/384km/init/x1.4002.init.$Y-$M-$D_$h.$m.$s.nc"
                  input_interval="initial_only" />

<immutable_stream name="static"
                  type="input"
                  filename_template="./Data/384km/init/x1.4002.init.$Y-$M-$D_$h.$m.$s.nc"
                  input_interval="initial_only" />

<immutable_stream name="restart"
                  type="input;output"
                  filename_template="./Data/384km/bg/restart.$Y-$M-$D_$h.$m.$s.nc"
                  input_interval="initial_only"
                  clobber_mode="overwrite"
                  output_interval="0_01:00:00" />

<stream name="output"
        type="input:output"
        filename_template="history.$Y-$M-$D_$h.$m.$s.nc"
        clobber_mode="overwrite"
        input_interval="initial_only"
        output_interval="0_01:00:00" >
	<file name="stream_list.atmosphere.output"/>
</stream>

<stream name="diagnostics"
        type="output"
        filename_template="diag.$Y-$M-$D_$h.$m.$s.nc"
        clobber_mode="overwrite"
        output_interval="none" >
	<file name="stream_list.atmosphere.diagnostics"/>
</stream>

<stream name="surface"
        type="none"
        filename_template="x1.4002.sfc_update.nc"
        filename_interval="none"
        input_interval="none" >
	<file name="stream_list.atmosphere.surface"/>
</stream>

<immutable_stream name="iau"
                  type="none"
                  filename_template="x1.4002.AmB.$Y-$M-$D_$h.$m.$s.nc"
                  filename_interval="none"
                  packages="iau"
                  input_interval="none" />
</streams>
